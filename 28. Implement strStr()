class Solution(object):
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        """
        
        

        """    
        if needle is None or needle =="":
            return 0;

        
        
        for i in range(len(haystack)):
            if haystack[i:i+len(needle)] == needle :
                return i;
        
        return -1
        """
    
        if needle not in haystack:
            return -1
        else:
            return haystack.index(needle)
