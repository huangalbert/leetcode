class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        
        if nums == []:
            return 0;
        
        result = 0
        for i in nums:
            if i != nums[result]:
                result+=1
                nums[result] = i;
                
            
        return result+1
